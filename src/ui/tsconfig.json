{
  "extends": "../../tsconfig-base",
  "compilerOptions": {
    // "rootDir": ".",
    "outDir": "../../dist/ui",
    "composite": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    // "types": [
    //   "webpack-env",
    //   "jest"
    // ],
    "paths": {
      "@/*": [
        "app/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "files": [
    "../types.ts"
  ],
  // "references": [
  //   { "path": "../common" }
  // ]
}
